{
	"version": "0.2",
	"ignorePaths": [
		"node_modules",
		"dist",
		"out",
		"build",
		"logs",
		"coverage",
		".git",
		".husky/_",
		".vscode/",
		".env",
		".env.local",
		".env.example",
		".env.development",
		".env.production",
		".env.test",
		".env.staging",
		".env.development.local",
		".env.production.local",
		".env.test.local",
		".env.staging.local",
		"public",
		"test",
		"tests",
		".tsbuildinfo",
		"generated",
		".gitignore",
		".prettierignore",
		"LICENSE",
		".next"
	],
	"languageSettings": [
		{
			"languageId": "*",
			"locale": "en, en-US"
		}
	],
	"dictionaries": [
		"git",
		"filetypes",
		"npm",
		"cspell-dictionary",
		"json",
		"typescript",
		"css",
		"shell",
		"html",
		"markdown",
		"yaml",
		"toml",
		"vue",
		"npm",
		"fonts",
		"node",
		"project-dependencies",
		"project-terms"
	],
	"dictionaryDefinitions": [],
	"files": ["**/*.{txt,md,json,js,cjs,mjs,ts,jsx,tsx,yml,yaml,toml}"],
	"words": [
		"API",
		"choosealicense",
		"dribbble",
		"embla",
		"Gumroad",
		"merch",
		"microsite",
		"Paywalled",
		"Podcasters",
		"rxaviers",
		"Sellfy",
		"shadcn",
		"shadcn/ui",
		"tseslint",
		"unsplash",
		"upsell",
		"upselling",
		"wavv"
	],
	"ignoreWords": ["Tensell", "ecommerce", "logomark"],
	"import": ["@cspell/dict-filetypes/cspell-ext.json", "@cspell/dict-typescript/cspell-ext.json"]
}
